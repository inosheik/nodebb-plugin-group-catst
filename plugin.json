{
	"id": "nodebb-plugin-group-catst",
	"url": "https://github.com/inosheik/nodebb-plugin-group-catst",
	"library": "./library.js",
	"hooks": [
		{ "hook": "static:app.load", "method": "init" },
		
        { "hook": "action:group.create", "method": "createGroup" },
        { "hook": "action:groups.destroy", "method": "destroyGroup" }
	],
	"staticDirs": {
		"static": "./static"
	},
	"less": [
		"static/style.less"
	],
	"scripts": [
		"static/lib/main.js"
	],
	"templates": "static/templates"
}
